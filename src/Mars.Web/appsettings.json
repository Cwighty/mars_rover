{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "UseUtcTimestamp": true,
        "TimestampFormat": "HH:mm:ss",
        "JsonWriterOptions": {
          "Indented":  true
        }
      }
    }
  },
  "AllowedHosts": "*",
  "SeqServer": "http://localhost:5341",
  "LokiServer": "http://loki:3100",
  "PATH_BASE": "/marsrover",
  "GameConfig": {
    "Password": "password",
    "ApiLimitPerSecond": "1000",
    "MaxMaps": "10",
    "DefaultMap": "1",
    "CleanupFrequencyMinutes": "60"
  }
}
